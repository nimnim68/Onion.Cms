@inject IResourceManager _resourceManager

<!doctype html>
<html class="no-js" lang="fa">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="بهترین قیمت خرید اینترنتی انواع شیر آلات سینک ظرفشویی، روشویی، توالت، حمام، ست کامل شیر آلات ایرانی و خارجی بهمراه مشخصات فنی کامل، نقد و بررسی، عکس،  فروش با گارانتی معتبر و پرداخت در محل - شیرجات ">
    <meta name="keywords" content="شیر آلات سینک ظرفشویی، روشویی، توالت، حمام، ست کامل شیر آلات،شیرآلات - خرید شیرالات راسان، شودر، شیبه و کی دبلیو سی، شیدر ">
    <title>@ViewBag.Title | فروشگاه اینترنتی شیرجات</title>
    <link rel="shortcut icon" type="image/x-icon" href="/img/favicon.ico">
    <link rel="alternate" href="https://shirjat.ir" hreflang="Fa" />

    <partial name="_PageStyles" />
    <script src="themes/autima/js/vendor/modernizr-3.7.1.min.js"></script>
    @await RenderSectionAsync("style", false)


</head>
<body>
    <partial name="_header" />
    @RenderBody()
    <partial name="_footer" />
    <partial name="_PageScripts" />
    @await RenderSectionAsync("Scripts", false)
</body>
</html>


